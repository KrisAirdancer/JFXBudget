<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <TableView fx:id="transactionsTable">
        <columns>
            <TableColumn fx:id="idCol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="dateCol" prefWidth="75.0" text="Date" />
          <TableColumn fx:id="payeeCol" prefWidth="75.0" text="Pyaee" />
            <TableColumn fx:id="categoryCol" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="noteCol" prefWidth="75.0" text="Note" />
            <TableColumn fx:id="amountCol" prefWidth="75.0" text="Amount" />
        </columns>
      </TableView>
      <Button fx:id="testButton" mnemonicParsing="false" onAction="#testMethod" text="Controller Test">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
